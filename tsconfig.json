{
  "compilerOptions": {
    "types": ["babel__generator"],
    "typeRoots": ["node_modules/@types", "path/to/your/custom/types"],
    "target": "es5",
    "baseUrl": ".",
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/src/*": ["src/*"]
    },
    // "jsx": "preserve",

  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
